# One-way ANOVA
# Importing the dataset
PlantGrowth <- PlantGrowth
# importing the dplyr library
# install.packages("dplyr")
library(dplyr)
# Computing the group means, sd, and se
stats <- PlantGrowth %>%
group_by(group) %>%
summarise(
mean = mean(weight),
sd = sd(weight),
se = sd(weight) / sqrt(n())
)
print(stats)
